[global_config]
  title_transmit_bg_color = "#c82829"
  title_inactive_bg_color = "#969896"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#1d1f21:#c82829:#718c00:#eab700:#4271ae:#8959a8:#3e999f:#d6d6d6:#969896:#c82829:#718c00:#eab700:#4271ae:#8959a8:#3e999f:#ffffff"
    foreground_color = "#d6d6d6"
    background_color = "#1d1f21"
    cursor_color = "#d6d6d6"

  [[Base16 Yesterday Dark]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#1d1f21:#c82829:#718c00:#eab700:#4271ae:#8959a8:#3e999f:#d6d6d6:#969896:#c82829:#718c00:#eab700:#4271ae:#8959a8:#3e999f:#ffffff"
    foreground_color = "#d6d6d6"
    background_color = "#1d1f21"
    cursor_color = "#d6d6d6"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Yesterday Dark
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
