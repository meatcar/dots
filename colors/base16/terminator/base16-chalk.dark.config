[global_config]
  title_transmit_bg_color = "#fb9fb1"
  title_inactive_bg_color = "#505050"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#151515:#fb9fb1:#acc267:#ddb26f:#6fc2ef:#e1a3ee:#12cfc0:#d0d0d0:#505050:#fb9fb1:#acc267:#ddb26f:#6fc2ef:#e1a3ee:#12cfc0:#f5f5f5"
    foreground_color = "#d0d0d0"
    background_color = "#151515"
    cursor_color = "#d0d0d0"

  [[Base16 Chalk Dark]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#151515:#fb9fb1:#acc267:#ddb26f:#6fc2ef:#e1a3ee:#12cfc0:#d0d0d0:#505050:#fb9fb1:#acc267:#ddb26f:#6fc2ef:#e1a3ee:#12cfc0:#f5f5f5"
    foreground_color = "#d0d0d0"
    background_color = "#151515"
    cursor_color = "#d0d0d0"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Chalk Dark
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
