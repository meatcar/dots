[global_config]
  title_transmit_bg_color = "#d54e53"
  title_inactive_bg_color = "#a7adba"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#343d46:#d54e53:#b9ca4a:#e7c547:#7aa6da:#c397d8:#70c0b1:#dfe1e8:#a7adba:#d54e53:#b9ca4a:#e7c547:#7aa6da:#c397d8:#70c0b1:#ffffff"
    foreground_color = "#65737e"
    background_color = "#ffffff"
    cursor_color = "#65737e"

  [[Base16 Yesterday Bright Light]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#ffffff:#d54e53:#b9ca4a:#e7c547:#7aa6da:#c397d8:#70c0b1:#dfe1e8:#a7adba:#d54e53:#b9ca4a:#e7c547:#7aa6da:#c397d8:#70c0b1:#343d46"
    foreground_color = "#65737e"
    background_color = "#ffffff"
    cursor_color = "#65737e"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Yesterday Bright Light
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
