[global_config]
  title_transmit_bg_color = "#3E9688"
  title_inactive_bg_color = "#2B685E"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#031A16:#3E9688:#883E96:#3E4C96:#96883E:#4C963E:#963E4C:#81B5AC:#2B685E:#3E9688:#883E96:#3E4C96:#96883E:#4C963E:#963E4C:#D2E7E4"
    foreground_color = "#81B5AC"
    background_color = "#031A16"
    cursor_color = "#81B5AC"

  [[Base16 Apathy Dark]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#031A16:#3E9688:#883E96:#3E4C96:#96883E:#4C963E:#963E4C:#81B5AC:#2B685E:#3E9688:#883E96:#3E4C96:#96883E:#4C963E:#963E4C:#D2E7E4"
    foreground_color = "#81B5AC"
    background_color = "#031A16"
    cursor_color = "#81B5AC"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Apathy Dark
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
