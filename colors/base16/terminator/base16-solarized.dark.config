[global_config]
  title_transmit_bg_color = "#dc322f"
  title_inactive_bg_color = "#657b83"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#002b36:#dc322f:#859900:#b58900:#268bd2:#6c71c4:#2aa198:#93a1a1:#657b83:#dc322f:#859900:#b58900:#268bd2:#6c71c4:#2aa198:#fdf6e3"
    foreground_color = "#93a1a1"
    background_color = "#002b36"
    cursor_color = "#93a1a1"

  [[Base16 Solarized Dark]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#002b36:#dc322f:#859900:#b58900:#268bd2:#6c71c4:#2aa198:#93a1a1:#657b83:#dc322f:#859900:#b58900:#268bd2:#6c71c4:#2aa198:#fdf6e3"
    foreground_color = "#93a1a1"
    background_color = "#002b36"
    cursor_color = "#93a1a1"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Solarized Dark
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
