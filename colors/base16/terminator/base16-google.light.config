    cursor_color = "#373b41"

  [[Base16 Google Light]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#ffffff:#CC342B:#198844:#FBA922:#3971ED:#A36AC7:#3971ED:#c5c8c6:#969896:#CC342B:#198844:#FBA922:#3971ED:#A36AC7:#3971ED:#1d1f21"
    foreground_color = "#373b41"
    background_color = "#ffffff"
    cursor_color = "#373b41"

[layouts]
  [[default]]
