[global_config]
  title_transmit_bg_color = "#ff0000"
  title_inactive_bg_color = "#808080"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#000000:#ff0000:#33ff00:#ff0099:#0066ff:#cc00ff:#00ffff:#d0d0d0:#808080:#ff0000:#33ff00:#ff0099:#0066ff:#cc00ff:#00ffff:#ffffff"
    foreground_color = "#d0d0d0"
    background_color = "#000000"
    cursor_color = "#d0d0d0"

  [[Base16 Isotope Dark]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#000000:#ff0000:#33ff00:#ff0099:#0066ff:#cc00ff:#00ffff:#d0d0d0:#808080:#ff0000:#33ff00:#ff0099:#0066ff:#cc00ff:#00ffff:#ffffff"
    foreground_color = "#d0d0d0"
    background_color = "#000000"
    cursor_color = "#d0d0d0"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Isotope Dark
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
