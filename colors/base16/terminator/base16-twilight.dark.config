[global_config]
  title_transmit_bg_color = "#cf6a4c"
  title_inactive_bg_color = "#5f5a60"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#1e1e1e:#cf6a4c:#8f9d6a:#f9ee98:#7587a6:#9b859d:#afc4db:#a7a7a7:#5f5a60:#cf6a4c:#8f9d6a:#f9ee98:#7587a6:#9b859d:#afc4db:#ffffff"
    foreground_color = "#a7a7a7"
    background_color = "#1e1e1e"
    cursor_color = "#a7a7a7"

  [[Base16 Twilight Dark]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#1e1e1e:#cf6a4c:#8f9d6a:#f9ee98:#7587a6:#9b859d:#afc4db:#a7a7a7:#5f5a60:#cf6a4c:#8f9d6a:#f9ee98:#7587a6:#9b859d:#afc4db:#ffffff"
    foreground_color = "#a7a7a7"
    background_color = "#1e1e1e"
    cursor_color = "#a7a7a7"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Twilight Dark
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
