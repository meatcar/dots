[global_config]
  title_transmit_bg_color = "#ef6155"
  title_inactive_bg_color = "#776e71"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#2f1e2e:#ef6155:#48b685:#fec418:#06b6ef:#815ba4:#5bc4bf:#a39e9b:#776e71:#ef6155:#48b685:#fec418:#06b6ef:#815ba4:#5bc4bf:#e7e9db"
    foreground_color = "#4f424c"
    background_color = "#e7e9db"
    cursor_color = "#4f424c"

  [[Base16 Paraiso Light]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#e7e9db:#ef6155:#48b685:#fec418:#06b6ef:#815ba4:#5bc4bf:#a39e9b:#776e71:#ef6155:#48b685:#fec418:#06b6ef:#815ba4:#5bc4bf:#2f1e2e"
    foreground_color = "#4f424c"
    background_color = "#e7e9db"
    cursor_color = "#4f424c"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Paraiso Light
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
