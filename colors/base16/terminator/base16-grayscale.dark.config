[global_config]
  title_transmit_bg_color = "#7c7c7c"
  title_inactive_bg_color = "#525252"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#101010:#7c7c7c:#8e8e8e:#a0a0a0:#686868:#747474:#868686:#b9b9b9:#525252:#7c7c7c:#8e8e8e:#a0a0a0:#686868:#747474:#868686:#f7f7f7"
    foreground_color = "#b9b9b9"
    background_color = "#101010"
    cursor_color = "#b9b9b9"

  [[Base16 Grayscale Dark]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#101010:#7c7c7c:#8e8e8e:#a0a0a0:#686868:#747474:#868686:#b9b9b9:#525252:#7c7c7c:#8e8e8e:#a0a0a0:#686868:#747474:#868686:#f7f7f7"
    foreground_color = "#b9b9b9"
    background_color = "#101010"
    cursor_color = "#b9b9b9"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Grayscale Dark
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
