[global_config]
  title_transmit_bg_color = "#cf6a4c"
  title_inactive_bg_color = "#666666"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#28211c:#cf6a4c:#54be0d:#f9ee98:#5ea6ea:#9b859d:#afc4db:#8a8986:#666666:#cf6a4c:#54be0d:#f9ee98:#5ea6ea:#9b859d:#afc4db:#baae9e"
    foreground_color = "#8a8986"
    background_color = "#28211c"
    cursor_color = "#8a8986"

  [[Base16 Bespin Dark]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#28211c:#cf6a4c:#54be0d:#f9ee98:#5ea6ea:#9b859d:#afc4db:#8a8986:#666666:#cf6a4c:#54be0d:#f9ee98:#5ea6ea:#9b859d:#afc4db:#baae9e"
    foreground_color = "#8a8986"
    background_color = "#28211c"
    cursor_color = "#8a8986"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Bespin Dark
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
