[global_config]
  title_transmit_bg_color = "#ff4136"
  title_inactive_bg_color = "#777777"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#111111:#ff4136:#2ecc40:#ffdc00:#0074d9:#b10dc9:#7fdbff:#bbbbbb:#777777:#ff4136:#2ecc40:#ffdc00:#0074d9:#b10dc9:#7fdbff:#ffffff"
    foreground_color = "#555555"
    background_color = "#ffffff"
    cursor_color = "#555555"

  [[Base16 Colors Light]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#ffffff:#ff4136:#2ecc40:#ffdc00:#0074d9:#b10dc9:#7fdbff:#bbbbbb:#777777:#ff4136:#2ecc40:#ffdc00:#0074d9:#b10dc9:#7fdbff:#111111"
    foreground_color = "#555555"
    background_color = "#ffffff"
    cursor_color = "#555555"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Colors Light
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
