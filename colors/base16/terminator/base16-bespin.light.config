[global_config]
  title_transmit_bg_color = "#cf6a4c"
  title_inactive_bg_color = "#666666"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#28211c:#cf6a4c:#54be0d:#f9ee98:#5ea6ea:#9b859d:#afc4db:#8a8986:#666666:#cf6a4c:#54be0d:#f9ee98:#5ea6ea:#9b859d:#afc4db:#baae9e"
    foreground_color = "#5e5d5c"
    background_color = "#baae9e"
    cursor_color = "#5e5d5c"

  [[Base16 Bespin Light]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#baae9e:#cf6a4c:#54be0d:#f9ee98:#5ea6ea:#9b859d:#afc4db:#8a8986:#666666:#cf6a4c:#54be0d:#f9ee98:#5ea6ea:#9b859d:#afc4db:#28211c"
    foreground_color = "#5e5d5c"
    background_color = "#baae9e"
    cursor_color = "#5e5d5c"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Bespin Light
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
